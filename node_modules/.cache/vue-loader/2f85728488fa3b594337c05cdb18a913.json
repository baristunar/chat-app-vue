unar/Desktop/chat-app-vue/src/components/login.vue","sourceRoot":"","sourcesContent":["<template>\n  <h1 class=\"text-center mt-5 text-size\">MESSENGER</h1>\n\n  <div>\n    \n    <div class=\"container centered\">\n      <div>\n        <form class=\"px-4 py-3\">\n          <div class=\"mb-3\">\n            <label class=\"form-label white text-light font-weight-bold\">Kullanıcı Adı</label>\n            <input\n              @keypress.enter=\"checkLogin\"\n              type=\"text\"\n              v-model=\"username\"\n              class=\"form-control\"\n            />\n          </div>\n          <div class=\"mb-3\">\n            <label class=\"form-label text-light font-weight-bold\">Şifre</label>\n            <input\n              @keypress.enter=\"checkLogin\"\n              type=\"password\"\n              v-model=\"pswrd\"\n              class=\"form-control\"\n            />\n          </div>\n          <div class=\"mb-3\"></div>\n        </form>\n        <button @click=\"checkLogin\" class=\"btn btn-danger centered button font-weight-bold\">\n          Giriş yap\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  emits:[\"user-info\", \"login\"],\n  data() {\n    return {\n      username: null,\n      pswrd: null,\n      userList: null,\n      status: true,\n    };\n  },\n\n  methods: {\n    checkLogin() {\n      if (this.pswrd == null || this.username == null) {\n        alert(\"Lütfen Tüm alanları Doldurunuz!\");\n      } else {\n        axios\n          .get(\n            \"http://localhost:3000/users?password=\"+this.pswrd+\"&username=\"+this.username\n          )\n          .then((res) => {\n            this.userList = res.data;\n            console.log(res);\n            if (this.userList.length > 0) {\n              this.status = false;\n              this.$emit(\"login\", this.status);\n              this.$emit(\"user-info\", this.userList);\n            } else {\n              alert(\"Hatalı kullanıcı adı/Şifre\");\n              this.status = true;\n            }\n          })\n          .catch((e) => {\n            console.log(\"e\", e);\n          });\n      }\n    },\n      \n  },\n};\n</script>\n<style scoped>\n.centered {\n  margin-top: 10%;\n}\n.centered button {\n  margin-left: 31%;\n  margin-top: -10%;\n}\n.text-size{\n  font-size: 56px !important;\n}\n\n</style>"]}]}